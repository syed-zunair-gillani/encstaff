{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    
    // "strict": true,
    // "noEmit": true,
    // "allowJs": true,
    // "jsx": "preserve",
